<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net462</TargetFrameworks>
		<RuntimeIdentifiers>win10-x64</RuntimeIdentifiers>
		<OutputType>Library</OutputType>
		<NoWarn>168;219;1701;1702;1705</NoWarn>
